{
  "name": "soma-fm-player",
  "version": "1.0.0",
  "description": "SPA audio player for the streaming stations provided by Soma FM.",
  "main": "index.js",
  "scripts": {
    "sass": "node-sass -w ./scss -o ./public/css",
    "sass-min": "node-sass -w ./scss -o ./public/css --output-style compressed",
    "init": "live-server --open=.",
    "generate": "nuxt generate",
    "deploy": "push-dir --dir=dist --branch=gh-pages --cleanup",
    "start": "npm run init"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rainner/soma-fm-player.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rainner/soma-fm-player/issues"
  },
  "homepage": "https://github.com/rainner/soma-fm-player#readme",
  "dependencies": {
    "gh-pages": "^2.0.1",
    "node-sass": "^4.9.3",
    "nuxt": "^2.2.0"
  },
  "devDependencies": {
    "push-dir": "^0.4.1"
  }
}
